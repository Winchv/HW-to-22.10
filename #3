#include <iostream>
using namespace std;
int main(){
int x, y1 = 0, y2 = 0;
cin » x;
int A[x];
for (int i = 0; i < x; i++){
cout « "input A[" « i+1 « "]:";
cin » A[i];
}
for (int i = 0; i < x; i++){
if (A[i]%27 == 0) {
if (A[i]%216 != 0)
y1 = y1 + A[i];
}
if (A[i]%72 == 0) {
if (A[i]%216 != 0)
y2 = y2 + A[i];
}
}
cout « " sum A[i]/27 == 0 :" « y1 « endl;
cout « " sum A[i]/72 == 0 :" « y2 « endl;
return 0;
delete []A;
}
